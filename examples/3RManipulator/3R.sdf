<?xml version="1.0" ?>
<sdf version="1.6">
    <model name="3rrr_manipulator">
        <static>false</static>

        <!-- Base link -->
        <link name="base_link">
            <inertial>
                <mass>1.0</mass>
                <inertia>
                    <ixx>0.1</ixx>
                    <iyy>0.1</iyy>
                    <izz>0.1</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
                <pose>0 0 0 0 0 0</pose>
            </inertial>
            <visual name="visual">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.1</size>
                    </box>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/Red</name>
                    </script>
                </material>
                <pose>0 0 0 0 0 0</pose>
            </visual>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.1</size>
                    </box>
                </geometry>
                <pose>0 0 0 0 0 0</pose>
            </collision>
        </link>

        <!-- Additional link -->
        <link name="link2">
            <inertial>
                <mass>0.5</mass>
                <inertia>
                    <ixx>0.05</ixx>
                    <iyy>0.05</iyy>
                    <izz>0.05</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
                <pose>0 0 0 0 0 0</pose>
            </inertial>
            <visual name="visual">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.1</size>
                    </box>
                </geometry>
                <material>
                    <script>
                        <name>Gazebo/Green</name>
                    </script>
                </material>
                <pose>0 0 0 0 0 0</pose>
            </visual>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.1 0.1 0.1</size>
                    </box>
                </geometry>
                <pose>0 0 0 0 0 0</pose>
            </collision>
        </link>

        <!-- Joints (joint1, joint2, and joint3) -->
        <joint name="joint1" type="revolute">
            <child>link2</child>
            <parent>base_link</parent>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-1.57</lower>
                    <upper>1.57</upper>
                    <effort>100</effort>
                    <velocity>0.1</velocity>
                </limit>
            </axis>
            <pose>0 0 0 0 0 0</pose>
        </joint>

        <!-- Define additional joints (joint2 and joint3) similarly -->
        <!-- ... -->

    </model>
</sdf>
